import java.util.Scanner;
public class oi8 {
    public static void main(String[] args){
        Scanner leia = new Scanner(System.in);

        System.out.println("Valor do deposito em poupança: ");
        double valor = leia.nextDouble();
        System.out.println("Valor após 1 mês de aplicação será de " + (valor * 0.013 + (valor)));
    }

}

//Receba o valor de um depósito em poupança. Calcule e mostre o valor após 1 mês de aplicação sabendo que rende 1,3% a.m
